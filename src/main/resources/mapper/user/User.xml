<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.test.sample.dao.UserDAO">

	<!-- 사용자 목록 조회 -->
	<select id="selectUserList" resultType="hashMap" parameterType="hashMap">
		SELECT *
		FROM TB_ENG_STM_USR
	</select>
	
	<!-- 사용자 목록 조회 -->
	<select id="selectUserVO" resultType="com.example.test.sample.vo.UserVO" parameterType="hashMap">
		SELECT USR_ID AS userId
			 , USR_NM AS userName
		FROM TB_ENG_STM_USR
	</select>
	
</mapper>